{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/block-link",
	"version": "0.1.0",
	"title": "Блок-ссылка",
	"category": "text",
	"icon": "menu",
	"description": "Example block scaffolded with Create Block tool.",
	"attributes": {
		"text": {
			"type": "string",
			"source": "html",
			"selector": "p"
		},
		"url": {
			"type": "string",
			"source": "attribute",
			"selector": "a",
			"attribute": "href",
			"default": "www.vsau.ru"
		},
		"blockType": {
			"type": "string",
			"default": "blue"
		},
		"withIcon": {
			"type": "boolean",
			"default": false
		}
	},
	"example": {},
	"supports": {
		"html": false
	},
	"textdomain": "vsau-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
